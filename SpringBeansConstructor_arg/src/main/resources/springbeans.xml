<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean class="com.entity.Student" id="stud">
		<constructor-arg index="0" value="1"></constructor-arg>
		<constructor-arg index="1" value="raju"></constructor-arg>
		<constructor-arg index="2" value="23"></constructor-arg>
		<constructor-arg index="3" value="karad"></constructor-arg>
		<constructor-arg index="4" ref="con"></constructor-arg>
	</bean>
	<bean class="com.entity.Country" id="con">
		<constructor-arg index="0" value="91"></constructor-arg>
		<constructor-arg index="1" value="India"></constructor-arg>

	</bean>
</beans>
